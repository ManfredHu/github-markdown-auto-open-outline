{
  "name": "Github markdown auto open outline",
  "description": "Github markdown auto open outline",
  "version": "1.0.0",
  "manifest_version": 3,
  "action": {
    "default_icon": "img/github-md-open.png"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["*://github.com/**/*.md"],
      "js": ["content.js"]
    }
  ]
}